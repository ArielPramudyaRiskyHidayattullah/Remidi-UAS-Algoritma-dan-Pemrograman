LIBRARY HARVEST !

def harvest_one_row():
    i = 0
    while i < 6:
        move()
        take()
        i += 1
    move()
def turn_right():
    turn_left()
    turn_left()
    turn_left()

LIBRARY HARVEST 2

def harvest_one_row():
    i = 0
    while i < 6:
        move()
        if object_here():
            while object_here():
                take()
        i += 1
    move()
def turn_right():
    turn_left()
    turn_left()
    turn_left()

LIBRARY HARVEST 3

def harvest_one_row():
    i = 0
    while i < 6:
        move()
        if object_here():
            while object_here():
                take()
            put()
        else:
            put()
        i += 1
    move()
def turn_right():
    turn_left()
    turn_left()
    turn_left()
